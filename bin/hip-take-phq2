#!/usr/bin/env python

from hippocrates.questionnaires.phq2.assessment import PHQ2Assessment


if __name__ == '__main__':
    assessment = PHQ2Assessment()
    assessment.take_assessment()
    print('Answers')
    print(assessment.display_answers())
    result = assessment.result()
    print('Analysis')
    print(assessment.display_result())
    print('Saving results.')
    assessment.save_results()
